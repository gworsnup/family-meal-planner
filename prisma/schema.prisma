generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Workspace {
  id           String   @id @default(cuid())
  slug         String   @unique
  name         String
  passcodeHash String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
